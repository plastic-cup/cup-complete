{
  "name": "autocomplete-cuppy",
  "version": "1.0.0",
  "description": "type a bit, get some nice words",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node test/backtest.js",
    "nocov": "node test/*.js",
    "coverage": "istanbul cover test/*.js && ./node_modules/.bin/istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "codeclimate": "CODECLIMATE_REPO_TOKEN=1c0b40358b6943d3ecd94d6254d5cd2ac5dfc51188b4019bfca83b9b8a3542e2 ./node_modules/codeclimate-test-reporter/bin/codeclimate.js < ./coverage/lcov.info",
    "jshint": "jshint -c .jshintrc --exclude-path .gitignore .",
    "start": "node server.js",
    "nodemon": "nodemon ./server.js"
  },
  "pre-commit": [
    "jshint",
    "coverage",
    "codeclimate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/plastic-cup/cup-complete.git"
  },
  "keywords": [
    "autocomplete",
    "english-dictionary"
  ],
  "author": "rjmk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/plastic-cup/cup-complete/issues"
  },
  "homepage": "https://github.com/plastic-cup/cup-complete",
  "devDependencies": {
    "istanbul": "^0.3.14",
    "pre-commit": "^1.0.7"
  },
  "dependencies": {
    "istanbul": "^0.3.14",
    "pre-commit": "^1.0.7"
  }
}
